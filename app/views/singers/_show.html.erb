<hr/>
<h3>Singer</h3>
<%= link_to 'Edit', controller: :singers, action: :edit %>
<table>
  <tr>
    <th>Background Vocals</th>
    <td><%= singer.musical_hobby.background_vocals %></td>
  </tr>
  <tr>
    <th>Range</th>
    <td><%= singer.range %></td>
  </tr>
  <tr>
    <th>Experience</th>
    <td><%= singer.musical_hobby.experience %></td>
  </tr>
  <tr>
    <th>Proficiency</th>
    <td><%= singer.musical_hobby.proficiency %></td>
  </tr>
</table>
