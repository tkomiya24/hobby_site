<hr/>
<h3>Singer</h3>
<% if @user.id == session[:user_id] %>
  <%= link_to 'Edit', controller: :singers, action: :edit %>
  <%= link_to 'Delete', controller: :singers, action: :destroy, method: :delete %>
<% end %>
<table>
  <tr>
    <th>Background Vocals</th>
    <td><%= user.singer.musical_hobby.background_vocals %></td>
  </tr>
  <tr>
    <th>Range</th>
    <td><%= user.singer.range %></td>
  </tr>
  <tr>
    <th>Experience</th>
    <td><%= user.singer.musical_hobby.experience %></td>
  </tr>
  <tr>
    <th>Proficiency</th>
    <td><%= user.singer.musical_hobby.proficiency %></td>
  </tr>
</table>
