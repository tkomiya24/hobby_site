<form>
  <input type="text"/>
</form>
<% @users.each do |user| %>
  <div>
    <div>
      <img src="http://placehold.it/150x150" class="img-thumbnail" />
      <h1><%= user.username %></h1>
    </div>
    <table>
      <tr>
        <th>First Name</th>
        <td><%= user.first_name %></td>
      </tr>
      <tr>
        <th>Last Name</th>
        <td><%= user.last_name %></td>
      </tr>
      <tr>
        <th>City</th>
        <td><%= user.city %></td>
      </tr>
      <tr>
        <th>E-mail</th>
        <td><%= user.email %></td>
      </tr>
    </table>
  </div>
<% end %>
