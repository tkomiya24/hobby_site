<form>
  <input type="text"/>
</form>
<ul class="search-results">
  <% @users.each do |user| %>
    <li class="search-result clearfix">
      <div class="result-profile">
        <div class="profile-picture">
          <img src="http://placehold.it/150x150" class="img-thumbnail" />
          <h1><%= user.username %></h1>
        </div>
        <table>
          <tr>
            <th>
              City
            </th>
          </tr>
          <tr>
            <td><%= user.city %></td>
          </tr>
        </table>
        <div class="instruments">
          <ul>
            <% user.musicians.each do |instrument| %>
              <li>
                <%= instrument.specific.class.name %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    <% end %>
    </li>
</ul>
